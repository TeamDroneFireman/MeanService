{
  "mongodb_dev": {
    "host": "mongo",
    "port": 27017,
    "database": "mongo-db1",
    "username": "admin",
    "password": "",
    "name": "mongodb_dev",
    "connector": "mongodb"
  },
  "userService": {
    "connector": "rest",
    "debug": "false",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://devprojetm2gla.istic.univ-rennes1.fr/api/SITUsers/{userId}/accessTokens/{token}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json",
            "Host": "user.docker.localhost"
          }
        },
        "functions": {
          "checkAuth": [
            "userId",
            "token"
          ]
        }
      }
    ]
  },
  "pushService": {
    "connector": "rest",
    "debug": "false",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "http://devprojetm2gla.istic.univ-rennes1.fr/api/pushMessages",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json",
            "Host": "push.docker.localhost"
          },
          "body": "{data}"
        },
        "functions": {
          "create": [
            "data"
          ]
        }
      }
    ]
  }
}
